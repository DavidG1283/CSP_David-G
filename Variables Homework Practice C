// David Garcia Variables Practice C

#include <stdio.h>

int main() {
    char my_name[] = "David";
    int number_between_1_and_10 = 7;
    int number_between_100_and_1000 = 354;
    char breakfast[] = "Pancakes with syrup";
    char favorite_color[] = "Blue";
    char school_name[] = "UCAS";
    int year = 2025;
    char eye_color[] = "Brown";
    int age = 20;
    char favorite_subject[] = "Math";

    printf("My name is: %s\n", my_name);
    printf("A number between 1 and 10: %d\n", number_between_1_and_10);
    printf("A number between 100 and 1000: %d\n", number_between_100_and_1000);
    printf("What I had for breakfast: %s\n", breakfast);
    printf("My favorite color is: %s\n", favorite_color);
    printf("The name of my school is: %s\n", school_name);
    printf("The current year is: %d\n", year);
    printf("My eye color is: %s\n", eye_color);
    printf("My age is: %d\n", age);
    printf("My favorite subject in school is: %s\n", favorite_subject);
    return 0;
}
