// David Garcia Expressions Practice C 

#include <stdio.h>
#include <math.h> 
int main() {
    printf("%f\n", 7 - 24.0 / 8 * 4 + 6);
    printf("%f\n", 18.0 / 3 - 7 + 2 * 5);
    printf("%f\n", 6 * 4.0 / 12 + 72.0 / 8 - 9);
    printf("%f\n", (17 - 6.0 / 2) + 4 * 3);
    printf("%f\n", -2 * (1 * 4 - 2.0 / 2) + (6 + 2 - 3));
    printf("%f\n", -1 * ((3 - 4 * 7) / 5.0) - 2 * 24.0 / 6);
    printf("%f\n", (3 * pow(5, 2) / 15) - (5 - pow(2, 2)));
    printf("%f\n", (pow(1, 4) * pow(2, 2) + pow(3, 3)) - pow(2, 5) / 4);
    printf("%f\n", (pow((22.0 / 2 - 2 * 5), 2)) + (4 - 6.0 / 6));
    return 0;
}
